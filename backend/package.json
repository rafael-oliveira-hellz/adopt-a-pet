{
  "name": "adopt-a-pet-api",
  "version": "1.0.0",
  "main": "src/server.js",
  "author": "Rafael de Oliveira (Hellz)",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p .",
    "start": "node build/dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "start:dev": "ts-node-dev \"src/index.ts\"",
    "start:local": "dotenv -e .env -- ts-node-dev --transpile-only src/index.ts | pino-pretty -c",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check \"src/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.3",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.2",
    "@types/express": "^4.17.13",
    "@types/express-flash": "^0.0.2",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "@types/express-pino-logger": "^4.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.41",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "nodemon": "^2.0.16",
    "ts-node": "10.6.0",
    "tsc": "^2.0.4",
    "typescript": "4.6.2",
    "eslint": "^7.32.0 || ^8.2.0",
    "pino-pretty": "^8.0.0",
    "prettier": "^2.6.2",
    "ts-node-dev": "^2.0.0"
  }
}
